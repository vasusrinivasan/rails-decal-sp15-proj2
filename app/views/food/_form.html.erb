<%= form_for(@food) do |f| %>
   <div class="name">
     <%= f.label :name %>
     <%= f.text_field :name %>
   </div>
   <div class="imageurl">
     <%= f.label :imageurl %>
     <%= f.text_field :imageurl %>
   </div>
   <div class="description">
     <%= f.label :description %>
     <%= f.text_field :description %>
   </div>
   <div class="actions">
     <%= f.submit %>
   </div>
<% end %>
